<?xml version='1.0'?>

<robot name="koala" xmlns:xacro="http://www.ros.org/wiki/xacro">



  <xacro:include filename="$(find koala_description)/urdf/koala.gazebo" />
  <xacro:include filename="$(find koala_description)/urdf/materials.xacro" />
  <xacro:include filename="$(find koala_description)/urdf/macros.xacro" />


  <link name='body'>
    <pose>0 0 0.1 0 0 0</pose>

    <inertial>
      <mass value="10.0"/>
      <origin xyz="0.0 0 0.1" rpy=" 0 0 0"/>
      <inertia
          ixx="0.5" ixy="0" ixz="0"
          iyy="1.0" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <geometry>
        <box size=".65 .35 .15"/>
      </geometry>
    </collision>

    <visual name='body_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".65 .35 .15"/>
      </geometry>
    </visual>


<!--     <collision name='caster_collision'>
      <origin xyz="-0.15 0 -0.05" rpy=" 0 0 0"/>
